<section class="results-container">

    {{#each artistsArr}}
    <div class="artist-container">
        <div class="img-container">
            <img src={{this.images.0.url}} alt={{this.name}}>
        </div>
        <div class="artist-footer">
            <h3>{{this.name}}</h3>
            <a href="#">View Albuns</a>
        </div>
    </div>
    {{else}}
    <h2>No artists found</h2>
    {{/each}}

</section>
